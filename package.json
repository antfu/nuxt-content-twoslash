{
  "name": "nuxt-content-twoslash",
  "type": "module",
  "version": "0.1.2",
  "packageManager": "pnpm@10.27.0",
  "description": "Enable TwoSlash for Nuxt Content",
  "license": "MIT",
  "repository": "antfu/nuxt-content-twoslash",
  "exports": {
    ".": "./dist/module.mjs"
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/module.d.mts"
      ]
    }
  },
  "bin": "./bin/nuxt-content-twoslash.mjs",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "prepack": "pnpm run build",
    "dev": "nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "release": "bumpp && pnpm publish",
    "lint": "eslint .",
    "test": "nr test:unit && nr test:e2e",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "typecheck": "nr dev:prepare && tsc --noEmit && cd playground && pnpm tsc --noEmit"
  },
  "peerDependencies": {
    "@nuxtjs/mdc": ">=0.5.0"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.2",
    "@shikijs/vitepress-twoslash": "^1.29.2",
    "ansis": "^4.2.0",
    "cac": "^6.7.14",
    "chokidar": "^5.0.0",
    "fast-glob": "^3.3.3",
    "minimatch": "^10.1.1",
    "pathe": "^2.0.3",
    "remark-parse": "^11.0.0",
    "shiki": "^1.29.2",
    "twoslash": "^0.2.12",
    "typescript": "~5.9.3",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@antfu/ni": "^28.1.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.23.0",
    "@nuxtjs/mdc": "0.9.5",
    "@playwright/test": "^1.57.0",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.10.4",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "playwright-core": "^1.57.0",
    "unbuild": "^3.6.1",
    "vitest": "^3.1.6"
  },
  "publishConfig": {
    "scripts": {},
    "devDependencies": {}
  }
}
