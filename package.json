{
  "name": "nuxt-content-twoslash",
  "type": "module",
  "version": "0.0.10",
  "packageManager": "pnpm@9.4.0",
  "description": "Enable TwoSlash for Nuxt Content",
  "license": "MIT",
  "repository": "antfu/nuxt-content-twoslash",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "bin": "./bin/nuxt-content-twoslash.mjs",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "prepack": "pnpm run build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "bumpp && pnpm publish",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "nr dev:prepare && tsc --noEmit"
  },
  "peerDependencies": {
    "@nuxtjs/mdc": ">=0.5.0"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.2",
    "@nuxt/schema": "^3.12.2",
    "@shikijs/vitepress-twoslash": "^1.9.1",
    "cac": "^6.7.14",
    "chokidar": "^3.6.0",
    "fast-glob": "^3.3.2",
    "pathe": "^1.1.2",
    "picocolors": "^1.0.1",
    "remark-parse": "^11.0.0",
    "shiki": "^1.9.1",
    "twoslash": "^0.2.9",
    "typescript": "^5.5.2",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.1",
    "@antfu/ni": "^0.21.12",
    "@nuxt/content": "^2.13.0",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.8.0",
    "@nuxt/test-utils": "^3.13.1",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.14.9",
    "bumpp": "^9.4.1",
    "eslint": "^9.5.0",
    "nuxt": "^3.12.2",
    "nuxt-content-twoslash": "workspace:*",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  },
  "publishConfig": {
    "scripts": {},
    "devDependencies": {}
  }
}
