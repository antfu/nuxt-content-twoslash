{
  "name": "nuxt-content-twoslash",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@8.15.1",
  "description": "Enable TwoSlash for Nuxt Content",
  "license": "MIT",
  "repository": "antfu/nuxt-content-twoslash",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "bin": "./bin/nuxt-content-twoslash.mjs",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "prepack": "pnpm run build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "bumpp && pnpm publish",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "peerDependencies": {
    "@nuxtjs/mdc": ">=0.5.0"
  },
  "dependencies": {
    "@nuxt/kit": "^3.10.1",
    "@nuxt/schema": "^3.10.1",
    "@shikijs/vitepress-twoslash": "^1.1.2",
    "cac": "^6.7.14",
    "fast-glob": "^3.3.2",
    "pathe": "^1.1.2",
    "picocolors": "^1.0.0",
    "remark-parse": "^11.0.0",
    "shiki": "^1.1.2",
    "twoslash": "^0.2.1",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@antfu/ni": "^0.21.12",
    "@nuxt/content": "^2.12.0",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/test-utils": "^3.11.0",
    "@types/mdast": "^4.0.3",
    "@types/node": "^20.11.17",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "nuxt": "^3.10.1",
    "nuxt-content-twoslash": "workspace:*",
    "vitest": "^1.2.2"
  },
  "publishConfig": {
    "scripts": {},
    "devDependencies": {}
  }
}
